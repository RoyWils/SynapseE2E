						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"Claim_df = spark.read.option(\"multiline\", \"false\").json(raw_location+\"Claim.ndjson\")\r\n",
					"Claim_df.write.mode(write_mode).parquet(processed_location+\"Claim/\")\r\n",
					"#display(Claim_df.limit(10))\r\n",
